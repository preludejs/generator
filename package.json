{
  "name": "@prelude/generator",
  "version": "4.7.0",
  "description": "Generator module.",
  "type": "module",
  "module": "./mjs/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    },
    "./*.js": {
      "import": "./mjs/*.js",
      "require": "./cjs/*.js"
    },
    "./*": {
      "require": "./cjs/*.js"
    }
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "make test",
    "preversion": "make test",
    "postversion": "make postversion",
    "ts-node": "node --no-warnings --loader ts-node/esm"
  },
  "keywords": [
    "prelude",
    "std",
    "lib",
    "stdlib",
    "library",
    "generators",
    "functional",
    "combinators"
  ],
  "author": "Mirek Rusin (https://github.com/mirek)",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/jest": "28.1.1",
    "eslint": "8.17.0",
    "jest": "28.1.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@prelude/cmp": "4.0.0"
  }
}
