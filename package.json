{
  "name": "@prelude/generator",
  "version": "9.0.0",
  "description": "Generator module.",
  "type": "module",
  "module": "./mjs/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    },
    "./*.js": {
      "import": "./mjs/*.js",
      "require": "./cjs/*.js"
    },
    "./*": {
      "require": "./cjs/*.js"
    }
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --coverage",
    "preversion": "make preversion",
    "postversion": "make postversion"
  },
  "keywords": [
    "prelude",
    "std",
    "lib",
    "stdlib",
    "library",
    "generators",
    "functional",
    "combinators"
  ],
  "author": "Mirek Rusin (https://github.com/mirek)",
  "license": "MIT",
  "devDependencies": {
    "@swc-node/register": "1.6.8",
    "@swc/core": "1.3.95",
    "@swc/jest": "0.2.29",
    "@tsconfig/node20": "20.1.4",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.9",
    "eslint": "8.52.0",
    "jest": "29.7.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@prelude/cmp": "4.1.0"
  }
}
